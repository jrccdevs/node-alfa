{
    "version": 2,
  
    "builds": [
        {
             "src": "./index.js", 
             "use": "@vercel/node" 
        }
    ],
  
    "routes": [
      {
        "src": "/api/imagenes/:path*",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "dest": "./controllers/imagenes.controllers.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "server": {
      "cors": {
        "origin": "https://reactaap.vercel.app"
      }
    }
    
  }